<div class="page-padding-bottom">
  <h1 class = "text-center profile-padding-y">Welcome <b>back</b></h1>
  <div class="profile-content-center">
    <div class="textbox-underline">
      <h3>Your profile</h3>
      <div>
        <h5><%= link_to "Edit Profile", edit_user_registration_path(current_user) %></h5>
      </div>
    </div>
    <div class="profileinfo-flex">
      <div>
        <h5>First and last name: </h5>
      </div>
      <div>
        <h5><%= current_user.first_name %> <%= current_user.last_name %></h5>
      </div>
    </div>

    <div class="profileinfo-flex">
      <div>
        <h5>Email address: </h5>
      </div>
      <div>
        <h5><%= current_user.email %></h5>
      </div>
    </div>

    <div class="profileinfo-flex">
      <div>
        <h5>Am I an owner? </h5>
      </div>
      <div>
      <% if current_user.owner_profile? %>
        <h5>Yes</h5>
      <% else %>
        <h5>No</h5>
      <% end %>
      </div>
    </div>

  </div>
</div>
